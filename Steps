    2  cd
    1  yum update -y
    3  sudo wget -O /etc/yum.repos.d/jenkins.repo     https://pkg.jenkins.io/redhat-stable/jenkins.repo
    4  rpm --import https://pkg.jenkins.io/redhat-stable/jenkins.io-2023.key
    5  sudo dnf install java-17-amazon-corretto -y
    6  java -version
    7  sudo yum install jenkins -y
    8  sudo systemctl enable jenkins
    9  sudo systemctl start jenkins
   
